
<section class="ftco-section ftco-section-more img" style="background-image: url({{ 'bg_5.jpg' | asset_url }});">
    <div class="container">
        <div class="row justify-content-center mb-3 pb-3">
      <div class="col-md-12 heading-section ftco-animate">
          <h2>{{ section.settings.title }}</h2>
      </div>
    </div>
    </div>
</section>

{% schema %}
 {
   "name": "Season Sale",
    "settings": [
        {
           "type": "text",
           "id": "title",
           "label": "Season Sale Title",
              "default": "Summer Sale"
        }
    
    ],
   "presets": [
       {
           
           "name":"Season Sale"
       }
   ]
 }
{% endschema %}