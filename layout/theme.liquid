<!doctype html>
<html class="no-js" lang="{{ request.locale.iso_code }}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">

    <title>
      {{ page_title }}
    </title>
     
      {% if page_description %}
        <meta name="description" content="{{ page_description | escape }}">
      {% endif %}
  
     



     
    {{ 'open-iconic-bootstrap.min.css' | asset_url | stylesheet_tag}}
    {{ 'animate.css' | asset_url | stylesheet_tag}}
    {{ 'owl.carousel.min.css' | asset_url | stylesheet_tag}}
    {{ 'owl.theme.default.min.css' | asset_url | stylesheet_tag}}
    {{ 'magnific-popup.css' | asset_url | stylesheet_tag}}
    {{ 'aos.css' | asset_url | stylesheet_tag}}
    {{ 'ionicons.min.css' | asset_url | stylesheet_tag}}
    {{ 'bootstrap-datepicker.css' | asset_url | stylesheet_tag}}
    {{ 'jquery.timepicker.css' | asset_url | stylesheet_tag}}
    {{ 'flaticon.css' | asset_url | stylesheet_tag}}
    {{ 'icomoon.css' | asset_url | stylesheet_tag}}

    
    {{ 'bootstrap.min.css' | asset_url  | stylesheet_tag }}

    
    {{ 'app.css' | asset_url  | stylesheet_tag }}
    

    {{ content_for_header }}
   
    <script>document.documentElement.className = document.documentElement.className.replace('no-js', 'js');</script>
  </head>

  <body data-aos-easing="slide" data-aos-duration="800" data-aos-delay="0">
    
     {% section 'header' %}
  
    {{ content_for_layout }}
    
   

    <!-- loader -->
  <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee"/><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00"/></svg></div>

    
    {{ 'jquery.min.js' | asset_url | script_tag }}

    
    {{ 'jquery-migrate-3.0.1.min.js' | asset_url | script_tag }}

    
    {{ 'popper.min.js' | asset_url | script_tag }}

    
    {{ 'bootstrap.min.js' | asset_url | script_tag }}

    
    {{ 'jquery.easing.1.3.js' | asset_url | script_tag }}

    {{ 'jquery.waypoints.min.js' | asset_url | script_tag }}

    {{ 'jquery.stellar.min.js' | asset_url | script_tag }}

    {{ 'owl.carousel.min.js' | asset_url | script_tag }}
    {{ 'jquery.magnific-popup.min.js' | asset_url | script_tag }}
    {{ 'aos.js' | asset_url | script_tag }}
    {{ 'jquery.animateNumber.min.js' | asset_url | script_tag }}
    {{ 'bootstrap-datepicker.js' | asset_url | script_tag }}
    {{ 'scrollax.min.js' | asset_url | script_tag }}

    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBVWaKrjvy3MaE7SQ74_uJiULgl1JY0H2s&sensor=false"></script>
    
    {{ 'google-map.js' | asset_url | script_tag }}


    {{ 'app.js' | asset_url | script_tag }}
    {{ content_for_footer }}
  </body>
</html>
